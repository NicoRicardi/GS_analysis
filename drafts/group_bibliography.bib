issn = {00027863},
journal = {Journal of the American Chemical Society},
number = {33},
pages = {10808--10818},
pmid = {16910676},
title = {{Color tuning in rhodopsins: The mechanism for the spectral shift between bacteriorhodopsin and sensory rhodopsin II}},
volume = {128},
year = {2006}
}
