@article{GarciaPrieto2011,
abstract = {The ASEP-MD method has been employed for studying the solvent effect on the conformational equilibrium of the alanine dipeptide in water solution. MP2 and density functional theory (DFT) levels of theory were used and results were compared. While in gas phase cyclic structures showing intramolecular hydrogen bonds were found to be the most stable, the stability order is reversed in water solution. Intermolecular interaction with the solvent causes the predominance of extended structures as the stabilizing contacts dipeptide-water are favoured. Free-energy differences in solution were calculated and PPII, R, and C5 conformers were identified as the most stable at MP2 level. Experimental data from Raman and IR techniques show discrepancies about the relative abundance of R y C5, our results support the Raman data. The DFT level of theory agrees with MP2 in the location and stability of PPII and R forms but fails in the location of C5. MP2 results suggest the possibility of finding traces of C7eq conformer in water solution, in agreement with recent experiments. {\textcopyright} 2011 American Institute of Physics.},
author = {Garc{\'{i}}a-Prieto, Francisco F. and Galv{\'{a}}n, Ignacio Fdez and Aguilar, Manuel A. and Martn, M. Elena},
doi = {10.1063/1.3658857},
issn = {00219606},
journal = {Journal of Chemical Physics},
number = {19},
pmid = {22112087},
title = {{Study on the conformational equilibrium of the alanine dipeptide in water solution by using the averaged solvent electrostatic potential from molecular dynamics methodology}},
volume = {135},
year = {2011}
}
