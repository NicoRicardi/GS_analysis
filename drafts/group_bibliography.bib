keywords = {Boron phosphide,Chemical shielding,Density functional theory,Nanotube},
number = {4},
pages = {902--924},
pmid = {20148191},
primaryClass = {physics},
title = {{Electron invariants and excited state structural analysis for electronic transitions within CIS, RPA, and TDDFT models}},
