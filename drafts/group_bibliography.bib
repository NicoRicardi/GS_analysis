@article{Genoni2018,
abstract = {Crystallography and quantum mechanics have always been tightly connected because reliable quantum mechanical models are needed to determine crystal structures. Due to this natural synergy, nowadays accurate distributions of electrons in space can be obtained from diffraction and scattering experiments. In the original definition of quantum crystallography (QCr) given by Massa, Karle and Huang, direct extraction of wavefunctions or density matrices from measured intensities of reflections or, conversely, ad hoc quantum mechanical calculations to enhance the accuracy of the crystallographic refinement are implicated. Nevertheless, many other active and emerging research areas involving quantum mechanics and scattering experiments are not covered by the original definition although they enable to observe and explain quantum phenomena as accurately and successfully as the original strategies. Therefore, we give an overview over current research that is related to a broader notion of QCr, and discuss options how QCr can evolve to become a complete and independent domain of natural sciences. The goal of this paper is to initiate discussions around QCr, but not to find a final definition of the field.},
author = {Genoni, Alessandro and Bu{\v{c}}insk{\'{y}}, Lukas and Claiser, Nicolas and Contreras-Garc{\'{i}}a, Julia and Dittrich, Birger and Dominiak, Paulina M. and Espinosa, Enrique and Gatti, Carlo and Giannozzi, Paolo and Gillet, Jean Michel and Jayatilaka, Dylan and Macchi, Piero and Madsen, Anders and Massa, Lou and Matta, Ch{\'{e}}rif F. and Merz, Kenneth M. and Nakashima, Philip N.H. and Ott, Holger and Ryde, Ulf and Schwarz, Karlheinz and Sierka, Marek and Grabowsky, Simon},
doi = {10.1002/chem.201705952},
issn = {15213765},
journal = {Chemistry - A European Journal},
keywords = {crystallography,quantum mechanics,scattering},
number = {43},
pages = {10881--10905},
title = {{Quantum Crystallography: Current Developments and Future Perspectives}},
volume = {24},
year = {2018}
}
@article{Heuser2018,
abstract = {We report the implementation of the Laplace-transform scaled opposite-spin (LT-SOS) resolution-of-the-identity second-order approximate coupled-cluster singles and doubles (RICC2) combined with frozen-density embedding for excitation energies and molecular properties. In the present work, we furthermore employ the Hartree-Fock density for the interaction energy leading to a simplified Lagrangian which is linear in the Lagrangian multipliers. This approximation has the key advantage of a decoupling of the coupled-cluster amplitude and multipliers, leading also to a significant reduction in computation time. Using the new simplified Lagrangian in combination with efficient wavefunction models such as RICC2 or LT-SOS-RICC2 and density-functional theory (DFT) for the environment molecules (CC2-in-DFT) enables the efficient study of biological applications such as the rhodopsin and visual cone pigments using ab initio methods as routine applications.},
author = {Heuser, Johannes and H{\"{o}}fener, Sebastian},
doi = {10.1063/1.5026651},
issn = {0021-9606},
journal = {J. Chem. Phys.},
month = {apr},
number = {14},
pages = {141101},
pmid = {29655359},
title = {{Communication: Biological applications of coupled-cluster frozen-density embedding}},
url = {http://aip.scitation.org/doi/10.1063/1.5026651},
volume = {148},
year = {2018}
}
