isbn = {0022-2267},
issn = {0066-426X},
journal = {Annu. Rev. Phys. Chem.},
keywords = {binding curves,dc-dft,electron affinity,embedding,error analysis,fragmentation methods,partition dft,potential energy surfaces},
month = {may},
